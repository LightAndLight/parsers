ghc-head:               True
no-tests-no-benchmarks: False
unconstrained:          False
allow-failures:         <7.3
hlint:                  True
hlint-options:          --cpp-ansi --cpp-define=HLINT
irc-channels:           irc.freenode.org#haskell-lens

constraint-set no-binary
  constraints: parsers -binary

constraint-set no-parsec
  constraints: parsers -parsec

constraint-set no-attoparsec
  constraints: parsers -attoparsec
